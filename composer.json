{
    "name": "mostafaznv/categories",
    "description": "Categories is a polymorphic Laravel package, for category management. You can categorize any eloquent model with ease, and utilize the power of Nested Sets, and the awesomeness of and Translatable models out of the box.",
    "keywords": [
        "model",
        "laravel",
        "eloquent",
        "category",
        "translatable",
        "categorizable",
        "polymorphic",
        "nested-set",
        "taxonomy",
        "mostafaznv"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mostafaznv/categories/issues",
        "source": "https://github.com/mostafaznv/categories",
        "docs": "https://github.com/mostafaznv/categories/blob/master/README.md"
    },
    "authors": [
        {
            "name": "mostafaznv",
            "email": "mostafa.zeinivand@gmail.com"
        },
        {
            "name": "mohammad mortazavi",
            "email": "s.mohammad@live.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "kalnoy/nestedset": "^5.0.3",
        "watson/validating": "^6.1.0",
        "spatie/laravel-translatable": "^5.0.0",
        "illuminate/database": "^8.36.0",
        "illuminate/support": "^8.36.0"
    },
    "require-dev": {
        "illuminate/container": "^8.36.0"
    },
    "autoload": {
        "psr-4": {
            "Mostafaznv\\Categories\\": "src/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Mostafaznv\\Categories\\CategoriesServiceProvider"
            ]
        }
    }
}
